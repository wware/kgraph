# First pass: Use LLM extraction to populate knowledge base from XML
uv run python -m examples.medlit.scripts.ingest \
  --input-dir examples/medlit/pmc_xmls \
  --output-dir medlit_bundle \
  --use-ollama \
  --use-llm-extraction

# Subsequent passes: Use embedding extraction (faster, once KB is populated)
uv run python -m examples.medlit.scripts.ingest \
  --input-dir examples/medlit/pmc_xmls \
  --output-dir medlit_bundle \
  --use-ollama  # Embedding extraction is default
