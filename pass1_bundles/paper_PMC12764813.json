      "paper_id": "paper_id",
      "text_span_id": "introduction_1",
      "text": "As a critical component of cancer control, cancer screening assumes a pivotal role in early detection, potentially saving lives by identifying cancers in their early stages or preventing their development altogether.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "cross-sectional survey",
      "eco_type": "questionnaire-based study",
