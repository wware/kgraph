  "evidence_entities": [
    {
      "id": "extracted_from_title:abstract:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "extracted_from_title",
      "text_span_id": "abstract_1",
      "text": "We recommend diagnostic tests for the exclusion of primary adrenal insufficiency in all patients with indicative clinical symptoms or signs.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "clinical practice guideline",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "extracted_from_title:abstract:2:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "extracted_from_title",
      "text_span_id": "abstract_2",
      "text": "We recommend a short corticotropin test (250 \u03bcg) as the 'gold standard' diagnostic tool to establish the diagnosis.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "clinical practice guideline",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "extracted_from_title:abstract:3:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "extracted_from_title",
      "text_span_id": "abstract_3",
      "text": "If a short corticotropin test is not possible in the first instance, we recommend an initial screening procedure comprising the measurement of morning plasma ACTH and cortisol levels.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "clinical practice guideline",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "extracted_from_title:abstract:4:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "extracted_from_title",
      "text_span_id": "abstract_4",
      "text": "Diagnosis of the underlying cause should include a validated assay of autoantibodies against 21-hydroxylase.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "clinical practice guideline",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "extracted_from_title:abstract:5:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "extracted_from_title",
      "text_span_id": "abstract_5",
      "text": "We recommend once-daily fludrocortisone (median, 0.1 mg) and hydrocortisone (15\u201325 mg/d) or cortisone acetate replacement (20\u201335 mg/d) applied in two to three daily doses in adults.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "clinical practice guideline",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "extracted_from_title:abstract:6:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "extracted_from_title",
      "text_span_id": "abstract_6",
      "text": "In children, hydrocortisone (\u223c8 mg/m2/d) is recommended.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "clinical practice guideline",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "extracted_from_title:abstract:7:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "extracted_from_title",
      "text_span_id": "abstract_7",
      "text": "This is also recommended for pregnant women with unexplained persistent nausea, fatigue, and hypotension.",
      "confidence": 0.9,
      "extraction_method": "llm",
