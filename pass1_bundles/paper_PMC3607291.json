  "evidence_entities": [
    {
      "id": "PMC3622825:abstract:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC3622825",
      "text_span_id": "abstract_1",
      "text": "The duration of excitatory responses to noxious UBD during acute colonic inflammation (3 days post-TNBS) was significantly shortened in the group with RTX pretreatment (25.37\u00b1.5 s, n=49) when compared to the control group (35.1\u00b14.2 s, n=43, p\u22640.05)",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "experimental",
      "eco_type": "animal_model",
      "source": "extracted"
    },
    {
      "id": "PMC3622825:results:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC3622825",
      "text_span_id": "results_1",
      "text": "Western blotting results provide evidence for a significant decrease in TRPV1 expression after intravesical treatment with RTX in the urothelium and DSM without changes in the distal colon",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "experimental",
      "eco_type": "animal_model",
      "source": "extracted"
    },
    {
      "id": "PMC3622825:results:2:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC3622825",
      "text_span_id": "results_2",
      "text": "Acute colitis (TNBS, 3 days), however, had no effect on the expression of TRPV1 in the urinary bladder, but increased the level of the protein in the distal colon by 2-fold",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "experimental",
      "eco_type": "animal_model",
      "source": "extracted"
    },
    {
      "id": "PMC3622825:results:3:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC3622825",
      "text_span_id": "results_3",
      "text": "Large somatic fields were found more frequently for viscerosomatic spinal neurons in rats pretreated with RTX in comparison to somatic fields mapped in vehicle treated animals (58% vs 23%, p\u22640.01)",
      "confidence": 0.9,
