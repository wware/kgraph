      "class": "Evidence",
      "entity_id": null,
      "paper_id": "paper_1",
      "text_span_id": "disc_2",
      "text": "SMRP are the only biomarkers for PM approved by the U.S. Food and Drug Administration (FDA)",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "bibliometric analysis",
      "eco_type": null,
