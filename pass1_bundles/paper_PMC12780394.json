      "entity_id": null,
      "paper_id": "paper_1",
      "text_span_id": "intro_3",
      "text": "The predominant risk factor for PM is asbestos exposure, with approximately 80% of cases linked to prior asbestos contact",
      "confidence": 0.95,
      "extraction_method": "llm",
