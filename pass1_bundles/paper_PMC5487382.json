  "evidence_entities": [
    {
      "id": "PMC5651043:abstract:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC5651043",
      "text_span_id": "abstract_1",
      "text": "Following BMSC transplantation, motoneurons extent of shrinkage were reduced and the animals' Basso, Beattie, and Bresnahan (BBB) locomotion scale scores were significantly higher at 21 and 28 days after injury when compared with the injured group.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "experimental",
      "eco_type": "animal_model",
      "source": "extracted"
    },
    {
      "id": "PMC5651043:abstract:2:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC5651043",
      "text_span_id": "abstract_2",
      "text": "Corresponding elevations in the expression of STAT3 and CNTF mRNA were observed in the hSCI + BMSCs group, and the levels were not significantly different from those observed in the sham group.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "experimental",
      "eco_type": "animal_model",
      "source": "extracted"
    },
    {
      "id": "PMC5651043:results:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC5651043",
      "text_span_id": "results_1",
      "text": "At days 21 and 28 after injury, the hSCI + BMSCs group had significantly higher BBB scores than did the hSCI alone group (p < 0.05).",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "experimental",
      "eco_type": "animal_model",
      "source": "extracted"
    },
    {
      "id": "PMC5651043:results:2:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC5651043",
      "text_span_id": "results_2",
      "text": "BMSC transplantation greatly reduced the density of GFAP staining, suggesting that transplanted BMSCs inhibited scar formation (p < 0.05).",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "experimental",
      "eco_type": "animal_model",
      "source": "extracted"
    },
    {
      "id": "PMC5651043:results:3:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC5651043",
      "text_span_id": "results_3",
      "text": "The CNTF and STAT3 mRNA levels in the hSCI + BMSC group were markedly upregulated (p < 0.05) compared to the hSCI alone group.",
