  "evidence_entities": [
    {
      "id": "PMC_CUSHING_COMORBIDITIES:introduction:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "intro_1",
      "text": "Cushing's disease is a rare disorder characterized by overproduction of ACTH from a pituitary adenoma leading to hypercortisolemia that in turn leads to increased morbidity and mortality.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:results:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "results_1",
      "text": "Recent evidence suggests that correction of hypercortisolemia may not lead to complete resolution of comorbidities associated with this condition. In particular, increased cardiovascular risk may persist despite long-term remission of hypercortisolemia.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:results:2:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "results_2",
      "text": "This may be related to persistence of visceral adiposity, adverse adipokine profile, glucose intolerance, hypertension, dyslipidemia, atherosclerosis and a procoagulant phenotype.",
      "confidence": 0.85,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:mortality:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "mortality_1",
      "text": "Multiple studies show that the standardized mortality ratio (SMR) is increased in CD (1.7\u20134.8), especially in patients with persistent hypercortisolism (3.7\u20134.2) compared to those in remission (1.8\u20133.17).",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:cardiovascular:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "cardiovascular_1",
      "text": "Cardiovascular risk is increased in CD patients even 5 years after remission. Compared with controls, CD patients had significantly higher waist-to-hip ratio, diastolic blood pressure, oral glucose tolerance test\u2014stimulated glucose and insulin levels, total/HDL cholesterol ratio and fibrinogen levels.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:hypertension:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "hypertension_1",
      "text": "Hypertension occurs in 55\u201385 % of CD patients, but resolves after remission in only 44\u201375 %.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:vte:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "vte_1",
      "text": "In a multicenter retrospective cohort study of 473 CS patients (360 CD), the incidence of VTE before treatment of hypercortisolemia was 14.6 per 1000 persons/year.",
      "confidence": 0.95,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:diabetes:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "diabetes_1",
      "text": "The prevalence of overt diabetes mellitus is 20\u201347 % and that of impaired glucose tolerance is 21\u201364 % in CD.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:osteoporosis:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "osteoporosis_1",
      "text": "The prevalence of fractures and osteoporosis as assessed by dual energy X-ray absorptiometry (DXA) has been reported to be 15\u201350 and 38\u201350 % respectively, and may be a presenting feature of CD.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:psychiatric:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "psychiatric_1",
      "text": "Most (54\u201385 %) patients with CD have some psychiatric disturbance. Depression and irritability are most common emotional lability, mania, paranoia, acute psychosis, anxiety and panic attacks also occur.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
      "source": "extracted"
    },
    {
      "id": "PMC_CUSHING_COMORBIDITIES:cognitive:1:llm",
      "class": "Evidence",
      "entity_id": null,
      "paper_id": "PMC_CUSHING_COMORBIDITIES",
      "text_span_id": "cognitive_1",
      "text": "Hippocampal atrophy was seen in 27 % of 12 active CS patients and correlated with performance on cognitive tests.",
      "confidence": 0.9,
      "extraction_method": "llm",
      "study_type": "review",
      "eco_type": null,
